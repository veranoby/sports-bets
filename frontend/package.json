{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 350",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "lint:commit": "eslint src --ext ts,tsx --fix --max-warnings 500",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,css}\" || true",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,css}\" || true",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.2",
    "@tailwindcss/vite": "^4.1.6",
    "@types/hls.js": "^0.13.3",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-share": "^3.0.3",
    "@videojs/http-streaming": "^3.17.2",
    "antd": "^5.27.3",
    "axios": "^1.9.0",
    "chart.js": "^4.4.9",
    "date-fns": "^4.1.0",
    "hls.js": "^1.6.13",
    "lucide-react": "^0.510.0",
    "patch-package": "^8.0.0",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.3",
    "react-quill-new": "^3.6.0",
    "react-router-dom": "^7.6.0",
    "react-share": "^5.2.2",
    "socket.io-client": "^4.8.1",
    "tailwindcss": "^4.1.6",
    "video.js": "^8.23.4",
    "videojs-contrib-hls": "^5.15.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/video.js": "^7.3.58",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  }
}
